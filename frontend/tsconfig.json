{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    // "paths": {
    //   "@assets/*": ["./src/assets/*"],
    //   "@components/*": ["./src/components/*"],
    //   "@hooks/*": ["./src/hooks/*"],
    //   "@pages/*": ["./src/pages/*"],
    //   "@store/*": ["./src/store/*"],
    //   "@types/*": ["./src/types/*"],
    //   "@utils/*": ["./src/utils/*"]
    // }
  },
  "include": [
    "src"
  ]
}
